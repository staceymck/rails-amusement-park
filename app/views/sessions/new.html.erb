<h1>Sign In</h1>
<%= form_tag '/signin' do %>
  <%= label_tag :name, "Your name" %>
  <%= collection_select(:user, :name, @users, :name, :name, prompt: "Select your username from dropdown") %><br>

  <%= label_tag :password, "Password:" %>
  <%= password_field_tag :password, nil %><br>

  <%= submit_tag "Sign In" %><br>
  <h1>Signup</h1>
  <%= link_to "Create an account", new_user_path %>
<% end %>